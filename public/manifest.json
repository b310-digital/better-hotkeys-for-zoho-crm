{
  "name": "Hello, World!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "build/background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "hello_extensions.png"
  },
  "commands": {
    "zoho-leads-open": {
      "suggested_key": {
        "default": "Alt+Shift+L",
        "mac": "Alt+Shift+L"
      },
      "description": "Open Leads module"
    }
  },
  "permissions": ["storage"]
}
